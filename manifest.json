{
  "manifest_version": 3,
  "name": "ClickTunes",
  "version": "1.0",
  "description": "A simple Chrome extension to make sounds on click.",
  "action": {
    "default_popup": "clickTunes.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  }
}
